<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- 色指定:サムネイル -->
    <SolidColorBrush x:Key="ThumbnailItemMouseOverBgColorBrush" Color="LightSkyBlue" />
    <SolidColorBrush x:Key="ThumbnailItemSelectBgColorBrush" Color="SteelBlue" />
    <SolidColorBrush x:Key="ThumbnailItemNameTextColorBrush" Color="Aqua" />

    <!-- 色指定:VS淡色 -->
    <SolidColorBrush x:Key="VsFgColorBrushLightRed" Color="#FF9D240C" />
    <SolidColorBrush x:Key="VsFgColorBrushLightGreen" Color="#FF368632" />
    <SolidColorBrush x:Key="VsFgColorBrushLightBlue" Color="#FF005098" />
    <SolidColorBrush x:Key="VsFgColorBrushLightYellow" Color="#FFDAB173" />

    <!-- 色指定:VS濃色 -->
    <SolidColorBrush x:Key="VsFgColorBrushDarkRed" Color="#FFF38B76" />
    <SolidColorBrush x:Key="VsFgColorBrushDarkGreen" Color="#FF8DD28A" />
    <SolidColorBrush x:Key="VsFgColorBrushDarkBlue" Color="#FF7AC1FF" />
    <SolidColorBrush x:Key="VsFgColorBrushDarkYellow" Color="#FFD7AC6A" />

</ResourceDictionary>
